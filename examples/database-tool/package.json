  "dependencies": {
    "@ai-spine/tools": "^1.0.0",
    "pg": "^8.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@ai-spine/tools-testing": "^1.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.17.0",
    "@types/pg": "^8.10.9",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
