{
  "name": "{{packageName}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "npm run clean && tsc",
    "build:rollup": "npm run clean && rollup -c",
    "build:webpack": "npm run clean && webpack --mode production",
    "build:vite": "npm run clean && vite build",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist coverage .nyc_output *.tsbuildinfo",
    "test": "{{#includeTests}}jest{{/includeTests}}{{^includeTests}}echo \"No tests specified\"{{/includeTests}}",
    "test:watch": "{{#includeTests}}jest --watch{{/includeTests}}{{^includeTests}}echo \"No tests specified\"{{/includeTests}}",
    "test:coverage": "{{#includeTests}}jest --coverage{{/includeTests}}{{^includeTests}}echo \"No tests specified\"{{/includeTests}}",
    "test:ci": "{{#includeTests}}jest --ci --coverage --watchAll=false{{/includeTests}}{{^includeTests}}echo \"No tests specified\"{{/includeTests}}",
    "lint": "eslint src --ext .ts,.js --fix",
    "lint:check": "eslint src --ext .ts,.js",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint:check && npm run format:check {{#includeTests}}&& npm run test:ci{{/includeTests}}",
    "prepare": "npm run build",
    "prepack": "npm run validate",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "schema": "curl -s http://localhost:3000/schema | jq '.' || echo 'Server not running or jq not installed'",
    "deploy": "echo \"Deployment not configured yet. See README.md for deployment options.\"",
    "docker:build": "{{#includeDocker}}docker build -t {{toolNameKebabCase}} .{{/includeDocker}}{{^includeDocker}}echo \"Docker not configured for this project\"{{/includeDocker}}",
    "docker:run": "{{#includeDocker}}docker run -p 3000:3000 {{toolNameKebabCase}}{{/includeDocker}}{{^includeDocker}}echo \"Docker not configured for this project\"{{/includeDocker}}",
    "update-deps": "npx npm-check-updates -u && npm install",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix"
  },
  "keywords": [
    "ai-spine",
    "tool",
    "api-integration",
    "{{toolNameKebabCase}}"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    {{#dependencies}}
    "{{{key}}}": "{{{value}}}"{{^isLast}},{{/isLast}}
    {{/dependencies}}
  },
  "devDependencies": {
    {{#devDependencies}}
    "{{{key}}}": "{{{value}}}"{{^isLast}},{{/isLast}}
    {{/devDependencies}}
  },
  "engines": {
    "node": ">=18.0.0"
  }
}