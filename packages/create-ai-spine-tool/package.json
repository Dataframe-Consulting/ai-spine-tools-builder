{
  "name": "create-ai-spine-tool",
  "version": "1.2.7",
  "description": "CLI tool for creating AI Spine compatible tools",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "bin": {
    "create-ai-spine-tool": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./cli": {
      "default": "./dist/cli.js"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest --passWithNoTests",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build && npm run build:verify"
  },
  "keywords": [
    "ai-spine",
    "tools",
    "cli",
    "scaffold",
    "generator"
  ],
  "author": "AI Spine Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "inquirer": "^8.2.6",
    "chalk": "^5.0.0",
    "ora": "^5.4.1",
    "fs-extra": "^11.0.0",
    "mustache": "^4.2.0",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.10",
    "@types/fs-extra": "^11.0.0",
    "@types/mustache": "^4.2.0",
    "@types/validate-npm-package-name": "^4.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "typescript": "^5.9.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup": "^4.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-progress": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
