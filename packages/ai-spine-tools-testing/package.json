{
  "name": "@ai-spine/tools-testing",
  "version": "1.2.9",
  "description": "Testing utilities for AI Spine tools",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c && node scripts/fix-types.js && npm run build:verify",
    "build:verify": "node scripts/verify-build.js",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:templates": "npm run build:ensure && node dist/template-validator.js",
    "test:examples": "npm run build:ensure && node dist/example-validator.js",
    "build:ensure": "node scripts/ensure-cli-files.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai-spine",
    "tools",
    "testing",
    "validation"
  ],
  "author": "AI Spine Team",
  "license": "MIT",
  "dependencies": {
    "@ai-spine/tools-core": "^1.2.9",
    "axios": "^1.6.0",
    "supertest": "^6.3.4",
    "fs-extra": "^11.0.0",
    "mustache": "^4.2.0",
    "chalk": "^5.0.0",
    "@faker-js/faker": "^8.4.1"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.2",
    "@types/jest": "^29.5.0",
    "@types/fs-extra": "^11.0.0",
    "@types/mustache": "^4.2.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.4.1",
    "rollup": "^4.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
